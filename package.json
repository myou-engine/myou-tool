{
  "name": "myou-tool",
  "version": "1.2.6",
  "description": "Command line utility for common operations for using the Myou game engine.",
  "main": "src/",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "node ./node_modules/coffeescript/bin/coffee -bco build src/",
    "local-install": "npm run prepublish && npm install -g ."
  },
  "bin": {
    "myou-tool": "./bin/myou-tool"
  },
  "keywords": [
    "myou",
    "engine",
    "cli",
    "server",
    "webpack",
    "boilerplate"
  ],
  "author": "Alberto Torres Ruiz",
  "license": "MIT",
  "devDependencies": {
    "coffeescript": "^2.5.1"
  },
  "dependencies": {
    "follow-redirects": "^1.11.0",
    "fs-extra": "^9.0.0",
    "mime": "^1.3.4",
    "unzipper": "^0.10.10",
    "validate-npm-package-license": "^3.0.1"
  },
  "coffeelintConfig": {
    "indentation": {
      "value": 4
    },
    "no_interpolation_in_single_quotes": {
      "level": "warn"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/myou-engine/myou-tool/"
  }
}
